{
  "name": "app",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "web": "npx browser-sync .",
    "web:test:env": "npx browser-sync . -w --server public --no-open --port 3300 --logLevel silent",
    "cli": "node ui/index.js",
    "api": "npm --prefix ../api run start",
    "test": "node --test test",
    "test:watch": "node --watch --test test",
    "test:debug": "node --experimental-vm-modules --inspect-brk node_modules/.bin/jest --runInBand --watchAll test/",
    "test:coverage": "node --test --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=lcov.info test/",
    "cy:web": "npx cypress open",
    "cy:headless": "npx cypress run",
    "test:e2e": "npx cross-env WEB_SERVER_URL=http://localhost:3300 npx concurrently \"npm run api\" \"npm run web:test:env\" \"npm run cy:headless\"",
    "test:e2e:web": "npx cross-env WEB_SERVER_URL=http://localhost:3300 npx concurrently \"npm run api\" \"npm run web:test:env\" \"npm run cy:web\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": "v20.11.0"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chalk": "^5.3.0",
    "chalk-table": "^1.0.2",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^19.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "browser-sync": "^2.29.3",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.6"
  }
}
